



build ./exports/combined/v1/values.json : z-run-out-in $
        ./exports/jhu/v1/series/values.json $
        ./exports/jhu/v1/daily/values.json $
        ./exports/ecdc/v1/worldwide/values.json $
        ./exports/nytimes/v1/us-states/values.json $
        ./exports/nytimes/v1/us-counties/values.json $
        | ./sources/generic-exports-combine.jq
    scriptlet = ':: tools / jq / filter / multi'
    arguments_pre = ':: process / generic / exports / combine / script'
    arguments = $
        --compact-output
    pool = cpu_heavy

build ./exports/combined/v1/values.tsv : z-run-in-out $
        ./exports/combined/v1/values.json $
        | ./sources/generic-exports-values-tsv.jq
    scriptlet = ':: tools / jq / filter'
    arguments_pre = ':: process / generic / exports / values / tsv / script'
    arguments = $
        --raw-output
    pool = cpu_moderate


build ./exports/combined/v1/values.json.gz : gzip ./exports/combined/v1/values.json
    pool = cpu_moderate

build ./exports/combined/v1/values.tsv.gz : gzip ./exports/combined/v1/values.tsv
    pool = cpu_moderate




build ./exports/combined/v1/values.sql-json : z-run-in-out $
        ./exports/combined/v1/values.json $
        | ./sources/generic-exports-values-sql.jq
    scriptlet = ':: tools / jq / filter'
    arguments_pre = ':: process / generic / exports / values / sql / script'
    arguments = $
        --compact-output
    pool = cpu_moderate

build ./exports/combined/v1/values-sqlite.sql : sql-from-json $
        ./exports/combined/v1/values.sql-json $
        | ./sources/sql-from-json.py
    format = sqlite
    pool = cpu_moderate

build ./exports/combined/v1/values-sqlite.db : sqlite-create $
        ./exports/combined/v1/values-sqlite.sql
    pool = cpu_moderate


build ./exports/combined/v1/values-sqlite.sql.gz : gzip ./exports/combined/v1/values-sqlite.sql
    pool = cpu_moderate

build ./exports/combined/v1/values-sqlite.db.gz : gzip ./exports/combined/v1/values-sqlite.db
    pool = cpu_moderate




build ./exports/combined/v1/locations-diff.json : z-run-out-in $
        ./imports/jhu/series/locations.json $
        ./imports/jhu/daily/locations.json $
        ./imports/ecdc/worldwide/locations.json $
        ./imports/nytimes/us-states/locations.json $
        ./imports/nytimes/us-counties/locations.json $
        | ./sources/generic-exports-locations-diff-json.jq
    scriptlet = ':: tools / jq / filter / multi'
    arguments_pre = ':: process / generic / exports / locations-diff / json / script'
    pool = cpu_moderate

build ./exports/combined/v1/locations-diff.tsv : z-run-in-out $
        ./exports/combined/v1/locations-diff.json $
        | ./sources/generic-exports-locations-diff-tsv.jq
    scriptlet = ':: tools / jq / filter'
    arguments_pre = ':: process / generic / exports / locations-diff / tsv / script'
    arguments = $
        --raw-output
    pool = cpu_moderate




build __combined__exports : phony $
        $
        ./exports/combined/v1/values.json $
        ./exports/combined/v1/values.tsv $
        ./exports/combined/v1/values-sqlite.sql $
        ./exports/combined/v1/values-sqlite.db $
        $
        ./exports/combined/v1/values.json.gz $
        ./exports/combined/v1/values.tsv.gz $
        ./exports/combined/v1/values-sqlite.sql.gz $
        ./exports/combined/v1/values-sqlite.db.gz $
        $
        ./exports/combined/v1/locations-diff.json $
        ./exports/combined/v1/locations-diff.tsv $
        ||

